<general:SafeWindow x:Class="UKSF.Old.Launcher.UI.Main.MainWindow"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:general="clr-namespace:UKSF.Old.Launcher.UI.General"
                    xmlns:main="clr-namespace:UKSF.Old.Launcher.UI.Main"
                    mc:Ignorable="d"
                    WindowStyle="None"
                    WindowStartupLocation="CenterScreen"
                    ResizeMode="NoResize"
                    AllowsTransparency="True"
                    Height="500"
                    Width="500"
                    Background="{DynamicResource Uksf.Background}"
                    BorderBrush="{DynamicResource Uksf.BackgroundLight}"
                    BorderThickness="1">
    <Grid Width="500" Height="500">
        <main:TitleBarControl x:Name="TitleBarControl" Margin="0,0,0,464" />
        <main:HomeControl x:Name="HomeControl" Margin="0,34,0,0" />
        <main:SettingsControl x:Name="SettingsControl" Margin="0,34,0,0" Visibility="Collapsed" />
    </Grid>
</general:SafeWindow>