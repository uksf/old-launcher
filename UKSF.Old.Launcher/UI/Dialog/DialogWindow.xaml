<general:SafeWindow x:Class="UKSF.Old.Launcher.UI.Dialog.DialogWindow"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:general="clr-namespace:UKSF.Old.Launcher.UI.General"
                    xmlns:dialog="clr-namespace:UKSF.Old.Launcher.UI.Dialog"
                    mc:Ignorable="d"
                    WindowStyle="None"
                    WindowStartupLocation="CenterScreen"
                    ResizeMode="NoResize"
                    AllowsTransparency="True"
                    Height="250"
                    Width="500"
                    Background="{DynamicResource Uksf.Background}"
                    BorderBrush="{DynamicResource Uksf.BackgroundLight}"
                    BorderThickness="1">
    <Grid>
        <dialog:DialogTitleBarControl x:Name="DialogTitleBarControl" Margin="0,0,0,214" />
        <dialog:DialogMainControl x:Name="DialogMainControl" Margin="0,36,0,0" />
    </Grid>
</general:SafeWindow>